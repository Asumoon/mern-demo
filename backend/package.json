{
  "name": "meanpro",
  "version": "0.0.1",
  "scripts": {
    "clean-server": "rimraf dist/server/*",
    "clean-public": "rimraf dist/public/*",
    "copy": "ts-node tools/copyAssets",
    "build-pro": "concurrently \"rimraf dist/server/*\" \"ts-node tools/copyAssets\" \"tsc -w -p server\"",
    "start": "node dist/server/app.js",
    "dev": "npm run copy && concurrently \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "pm2": "pm2 start pm2.config.js"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.3.0",
    "body-parser": "^1.19.2",
    "composable-middleware": "^0.3.0",
    "concurrently": "^7.0.0",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.21.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.2",
    "gridfs-stream": "^1.1.1",
    "hammerjs": "^2.0.8",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.8.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "nanoid": "^3.3.1",
    "node-fetch": "^3.2.3",
    "npm": "^7.20.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rand-token": "^1.0.1",
    "shortid": "^2.2.16",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^1.10.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "codelyzer": "~4.3.0",
    "shelljs": "^0.8.5",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^4.6.3"
